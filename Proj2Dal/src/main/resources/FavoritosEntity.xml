<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="https://jakarta.ee/xml/ns/persistence/orm"
                 version="3.0">

    <entity class="com.example.proj2dal.Entity.FavoritosEntity">
        <table name="favoritos" schema="public" catalog="projeto"/>
        <attributes>
            <id name="idFavoritos">
