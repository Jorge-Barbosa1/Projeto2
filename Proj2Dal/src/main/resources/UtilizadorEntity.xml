<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="https://jakarta.ee/xml/ns/persistence/orm"
                 version="3.0">

    <entity class="com.example.proj2dal.Entity.UtilizadorEntity">
        <table name="utilizador" schema="public" catalog="projeto"/>
        <attributes>
            <id name="idUser">
                <column name="id_user"/>
            </id>
            <basic name="email">
                <column name="email"/>
            </basic>
            <basic name="telefone">
                <column name="telefone"/>
            </basic>
            <basic name="nome">
                <column name="nome"/>
            </basic>
            <basic name="username">
                <column name="username"/>
            </basic>
            <basic name="password">
                <column name="password"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
