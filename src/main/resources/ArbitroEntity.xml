<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="https://jakarta.ee/xml/ns/persistence/orm"
                 version="3.0">

    <entity class="Entity.ArbitroEntity">
        <table name="arbitro" schema="public" catalog="projeto"/>
        <attributes>
            <id name="idArbitro">
                <column name="id_arbitro"/>
            </id>
            <basic name="nome">
                <column name="nome"/>
            </basic>
            <basic name="idContacto">
                <column name="id_contacto"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
