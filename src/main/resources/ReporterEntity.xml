<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="https://jakarta.ee/xml/ns/persistence/orm"
                 version="3.0">

    <entity class="Entity.ReporterEntity">
        <table name="reporter" schema="public" catalog="projeto"/>
        <attributes>
            <id name="idReporter">
                <column name="id_reporter"/>
            </id>
            <basic name="idJogo">
                <column name="id_jogo"/>
            </basic>
            <basic name="idContactos">
                <column name="id_contactos"/>
            </basic>
            <basic name="nome">
                <column name="nome"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
