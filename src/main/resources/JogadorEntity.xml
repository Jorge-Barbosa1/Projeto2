<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="https://jakarta.ee/xml/ns/persistence/orm"
                 version="3.0">

    <entity class="Entity.JogadorEntity">
        <table name="jogador" schema="public" catalog="projeto"/>
        <attributes>
            <id name="idJogador">
                <column name="id_jogador"/>
            </id>
            <basic name="nome">
                <column name="nome"/>
            </basic>
            <basic name="idContactos">
                <column name="id_contactos"/>
            </basic>
            <basic name="idEquipa">
                <column name="id_equipa"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
